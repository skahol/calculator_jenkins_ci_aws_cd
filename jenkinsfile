pipeline{
    agent none
    stages {
        stage('test-case'){
            agent { docker 'python:3.8' }
            steps{
                sh 'python ./calcu/test.py'
            }
        }
        stage('build-package'){
            agent { docker 'ubuntu:latest' }
            steps{
                sh 'sam build'
            }
        }
    }
}